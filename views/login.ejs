<%- include('partials/header.ejs')%>
<body id="login-body">
	<a href="/">Home</a>
	<h1>Welcome back to GuitarPal!</h1>
	<h2>Complete the form below to login to your account.</h2>
	<br /><br />
	<div class="login-container">
		<form action="/login" method="post">
			<div class="email-container">
				Email:<br />
				<input
					type="email"
					name="email"
					placeholder="Email"
					required
				/><br /><br />
			</div>
			<div class="password-container">
				Password:<br />
				<input
					type="password"
					name="password"
					placeholder="Password"
					required
				/><br /><br />
			</div>
			<div class="button-container">
				<input type="submit" value="Login" class="button1" />
				<button class="button1"><a href="/register">Register</a></button>
			</div>
		</form>
	</div>
</body>
